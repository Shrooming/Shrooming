    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def classify_expression(img_path):\\n\",\n",
    "    \"    symbols = segment_expression(img_path, img_size=45, show_steps=True)\\n\",\n",
    "    \"    predicted_chars = [predict_symbol_rf_hog(s) for s in symbols]\\n\",\n",
    "    \"    return \\\"\\\".join(predicted_chars)\\n\",\n",
    "    \"\\n\",\n",
    "    \"def solve_expression(expr):\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        return sympify(expr).evalf()\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        return f\\\"Error: {e}\\\"\"\n",
    "   ],\n",
    "   \"id\": \"3a4b6e1cdd648d57\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 27\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-09-18T13:58:42.555429Z\",\n",
    "     \"start_time\": \"2025-09-18T13:58:42.550628Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Predicting with hog instead of just RF\\n\",\n",
    "    \"def predict_symbol_rf_hog(img):\\n\",\n",
    "    \"    img = img.squeeze()\\n\",\n",
    "    \"    hog_feat = hog(\\n\",\n",
    "    \"        img,\\n\",\n",
    "    \"        orientations=9,\\n\",\n",
    "    \"        pixels_per_cell=(4, 4),\\n\",\n",
    "    \"        cells_per_block=(2, 2),\\n\",\n",
    "    \"        block_norm=\\\"L2-Hys\\\"\\n\",\n",
    "    \"    ).reshape(1, -1)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    pred_idx = rf_hog.predict(hog_feat)[0]\\n\",\n",
    "    \"    return index_to_label[pred_idx]\"\n",
    "   ],\n",
    "   \"id\": \"99250f664ed7dc3e\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 28\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-09-18T13:58:45.099961Z\",\n",
    "     \"start_time\": \"2025-09-18T13:58:45.091608Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
