    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Train/test split\\n\",\n",
    "    \"X_train, X_test, y_train, y_test = train_test_split(X_all, y_all, test_size=0.2, random_state=42)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Extract HOG features\\n\",\n",
    "    \"def extract_hog_features(images):\\n\",\n",
    "    \"    features = []\\n\",\n",
    "    \"    for img in images:\\n\",\n",
    "    \"        img = img.squeeze()  # 45x45\\n\",\n",
    "    \"        hog_feat = hog(\\n\",\n",
    "    \"            img,\\n\",\n",
    "    \"            orientations=9,\\n\",\n",
    "    \"            pixels_per_cell=(4,4),\\n\",\n",
